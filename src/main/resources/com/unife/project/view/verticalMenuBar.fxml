<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="verticalRoot" alignment="TOP_LEFT" spacing="10" style="-fx-background-color: #ff6600;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.unife.project.controller.VerticalMenuBarController">
    <children>
        <Button fx:id="homeButtonV" blendMode="MULTIPLY" maxHeight="1.7976931348623157E308" onAction="#handleGoHome" prefWidth="85.0" style="-fx-border-color: white;" text="Home">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <Button fx:id="adminButtonV" blendMode="MULTIPLY" onAction="#handleGoAdmin" prefWidth="85.0" style="-fx-border-color: white;" text="Admin" />
        <Button fx:id="piantagioneButtonV" blendMode="MULTIPLY" onAction="#handleGoPiantagione" prefWidth="85.0" style="-fx-border-color: white;" text="Piantagione" />
        <Button fx:id="pastoreButtonV" blendMode="MULTIPLY" onAction="#handleGoPastore" prefWidth="85.0" style="-fx-border-color: white;" text="Pastore" />
        <!-- Aggiungi altri bottoni qui -->
    </children>
</VBox>
